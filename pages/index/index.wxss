page {
  background: #f1f1f1;
}

.top {
  display: flex;
  height: 106rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.top .top_left {
  display: flex;
  margin-right: 21rpx;
  flex: 1;
}

.top .top_left image {
  width: 36rpx;
  height: 46rpx;
  margin-top: 28rpx;
}

.top .top_left view {
  height: 28rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(52, 52, 52, 1);
  margin-left: 11rpx;
  line-height: 106rpx;
}

.top .top_input {
  position: relative;
  margin-top: 18rpx;
}

.top .top_input .input {
  width: 400rpx;
  height: 66rpx;
  border: 1px solid rgba(106, 106, 106, 0.3);
  border-radius: 33rpx;
  padding-left: 70rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.top .top_input image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 20rpx;
  top: 18rpx;
}

.top .top_right {
  display: flex;
}

.top .top_right .top_right_img {
  position: relative;
}

.top .top_right .top_right_img image {
  width: 41rpx;
  height: 46rpx;
  margin-left: 27rpx;
  margin-top: 28rpx;
}

.top .top_right>image {
  width: 41rpx;
  height: 46rpx;
  margin-top: 28rpx;
  margin-left: 39rpx;
}

.top .top_right .top_right_img .top_right_yuan {
  width: 34rpx;
  height: 24rpx;
  background: rgba(255, 0, 0, 1);
  border: 1px solid rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 24rpx;
  position: absolute;
  top: 14rpx;
  right: -24rpx;
}

.img_oo image {
  width: 750rpx;
  height: 360rpx;
}

.item {
  display: flex;
  padding: 0 20rpx;
  margin-top: 30rpx;
  justify-content: space-between;
  flex-wrap: wrap;
}

.item .item_left {
  width: 48%;
}

.item .item_right {
  width: 48%;
}

.item .list {
  position: relative;
  background: #fff;
  border-radius: 10rpx;
  width: 348rpx;
  margin-top: 20rpx;
}

.item .list .img {
  height: 486rpx;
  width: 348rpx;
}

.item .list .img1 {
  width: 348rpx;
  height: 475rpx;
}
.item .list .img11{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top:15rpx;
  right: 15rpx;
}

.item .list .list_title {
  width: 348rpx;
  height: 92rpx;
  padding: 15rpx 22rpx 0rpx 22rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.item .list .list_footer {
  padding: 0 22rpx;
  box-sizing: border-box;
  margin-top: 12rpx;
  display: flex;
  justify-content: space-between;
}

.item .list .list_footer .list_la {
  height: 60rpx;
  display: flex;
}

.item .list .list_footer .list_la image {
  width: 40rpx;
  height: 40rpx;
}

.item .list .list_footer .list_la view {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(120, 120, 120, 1);
  line-height: 45rpx;
  margin-left: 12rpx;
}
.item .list .list_footer .Fabulous{
  width: 80rpx;
  display: flex;
  justify-content: space-between;
}
.item .list .list_footer .Fabulous image{
  width: 26rpx;
  height: 22rpx;
  margin-top: 12rpx;
}
.item .list .list_footer .Fabulous view{
  font-size:24rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(0,0,0,1);
line-height: 41rpx;
}
.item .list .Popular {
  position: absolute;
  top: 0rpx;
  left: 50%;
  width: 300rpx;
  transform: translate(-50%);
}

.item .list .Popular .Popular_list {
  height: 76rpx;
  line-height: 76rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

.item .list .Popular .Popular_list1 {
  font-size: 28rpx;
  height: 76rpx;
  line-height: 76rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  border-bottom: 1px solid rgba(255, 255, 255, 0.3);
}

.item .list .Popular view:nth-child(6) {
  border: 0 !important;
}
