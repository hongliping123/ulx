.top {
  width: 690rpx;
  height: 56rpx;
  margin: 0 auto;
  display: flex;
}

.top .top_left {
  display: flex;
}

.top .top_left image {
  width: 27rpx;
  height: 35rpx;
  margin-top: 16rpx;
}

.top .top_left text {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(52, 52, 52, 1);
  line-height: 56rpx;
  margin-left: 10rpx;
  margin-top: 4rpx;
}

.top .top_conter {
  width: 400rpx;
  height: 56rpx;
  position: relative;
  margin-left: 30rpx;
}

.top .top_conter input {
  width: 400rpx;
  height: 56rpx;
  border: 1px solid rgba(106, 106, 106, 1);
  border-radius: 28rpx;
  padding-left: 64rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  padding-right: 20rpx;
  box-sizing: border-box;
}

.top .top_conter image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 14rpx;
  left: 20rpx;
}

.top .top_right {
  flex: 1;
  margin-left: 38rpx;
  display: flex;
}

.top .top_right .tan {
  position: relative;
}

.top .top_right .tan .tan_img {
  width: 267rpx;
  background: red;
  position: absolute;
  top: 62rpx;
  left: -180rpx;
  background: rgba(46, 46, 46, 1);
  border-radius: 10rpx;
  padding-bottom: 20rpx;
}

.top .top_right .tan .tan_img .tan_img_list {
  width: 267rpx;
  height: 60rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 60rpx;
}

.top .top_right .top_right_xx {
  position: relative;
}

.top .top_right .top_right_xx image {
  width: 31rpx;
  height: 35rpx;
  margin-top: 12rpx;
}

.top .top_right .top_right_xx view {
  width: 34rpx;
  height: 23rpx;
  background: rgba(255, 0, 0, 1);
  border: 1px solid rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  line-height: 23rpx;
  text-align: center;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  position: absolute;
  top: -2rpx;
  right: -22rpx;
}

.top .top_right .duo {
  width: 40rpx;
  height: 35rpx;
  margin-top: 10rpx;
  margin-left: 40rpx;
}

.top .top_right .jian {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 50rpx;
  left: 44rpx;
}

.nav {
  display: flex;
  margin: 0 auto;
  margin-top: 25rpx;
  width: 690rpx;
}

.nav .nav_list {
  height: 32rpx;
  position: relative;
  line-height: 32rpx;
  margin-right: 40rpx;
  display: flex;
  text-align: center;
}

.nav .nav_list image {
  width: 74rpx;
  height: 8rpx;
  position: absolute;
  left: 50%;
  bottom: -2rpx;
  transform: translateX(-50%);
  z-index: -1;
}

.nav .nav_list text {
  width: 80rpx;
  height: 32rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(29, 32, 35, 1);
  line-height: 32rpx;
  margin-right: 40rpx;
  margin: 0 auto;
}

.nav .nav_list .text1 {
  width: 80rpx;
  height: 32rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(29, 32, 35, 1);
  line-height: 32rpx;
  margin-right: 40rpx;
  margin: 0 auto;
  font-weight: bold;
}

.item {
  margin: 0 auto;
  margin-top: 35rpx;
  width: 690rpx;
}

.item .item_list {
  border-bottom: 1px solid #ccc;
  padding-bottom: 30rpx;
  margin-top: 34rpx;
}

.item .item_list .item_list_title {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.item .item_list .item_send {
  display: flex;
  margin-top: 22rpx;
}

.item .item_list .item_send .item_send_left {
  margin-right: 35rpx;
}

.item .item_list .item_send .item_send_left .item_send_left_name {
  display: flex;
}

.item .item_list .item_send .item_send_left .item_send_left_name image {
  width: 54rpx;
  height: 54rpx;
}

.item .item_list .item_send .item_send_left .item_send_left_name text {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 54rpx;
  margin-left: 27rpx;
}

.item .item_list .item_send .item_send_left .item_send_left_text {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(145, 145, 145, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  margin-top: 26rpx;
}

.item .item_list .item_send .item_send_right {
  flex: 1;
}

.item .item_list .item_send .item_send_right image {
  width: 253rpx;
  height: 168rpx;
  margin-top: 14rpx;
}

.item .item_list .item_list_footer {
  height: 23rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(205, 205, 205, 1);
  margin-top: 20rpx;
}

