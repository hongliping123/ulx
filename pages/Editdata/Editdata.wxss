page {
  background: #f5f5f5;
}

.cont_top {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  padding: 20rpx 20rpx;
}

.cont_body {
  background: #fff;
}

.cont_body_title {
  padding: 0 20rpx;
}

.cont_body_img {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #eee;
  height: 150rpx;
}

.cont_body_img image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}

.cont_body_img text {
  font-size: 28rpx;
  color: rgba(153, 153, 153, 1);
}

.cont_body_text_img image {
  width: 12rpx;
  height: 21rpx;
  margin-left: 20rpx;
}

.cont_body_text_img_sex {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.cont_body_text {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.cont_body_text1 {
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.sex_left {
  margin-right: 88rpx;
}

.cont_but {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 690rpx;
  height: 90rpx;
  background: rgba(226, 85, 32, 1);
  border-radius: 10rpx;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  margin: 0 auto;
  margin-top: 60rpx;
}
