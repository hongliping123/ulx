page {
  background: #f5f5f5;
}

.top {
  display: flex;
  width: 750rpx;
  background: #fff;
  padding-top: 23rpx;
  padding-bottom: 36rpx;
}

.top .top_list {
  flex: 1;
  display: flex;
  justify-content: center;
  border-right: 2px solid #d2d2d2;
  overflow: hidden;
}

.top .top_list:nth-child(3) {
  border-right: 0;
}

.top .top_list text {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(1, 1, 1, 1);
  line-height: 33rpx;
}

.top .top_list image {
  width: 21rpx;
  height: 12rpx;
  margin-top: 12rpx;
  margin-left: 33rpx;
}

.mian {
  display: flex;
  width: 750rpx;
}

.mian .mian_left {
  width: 134rpx;
}

.mian .mian_left .mian_left_list {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: rgba(126, 126, 126, 1);
}

.mian .mian_left .mian_left_list1 {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: rgba(226, 85, 32, 1);
  background: #fff;
}

.mian .mian_right {
  flex: 1;
  background: #fff;
}

.mian .mian_right .mian_right_item {
  width: 100%;
  padding-left: 15rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.mian .mian_right .mian_right_item .mian_right_list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.mian .mian_right .mian_right_item .mian_right_list .mian_right_list_img {
  position: relative;
  margin-bottom: 40rpx;
  width: 270rpx;
  height: 270rpx;
}

.mian .mian_right .mian_right_item .mian_right_list .mian_right_list_img image {
  width: 270rpx;
  height: 270rpx;
}

.mian .mian_right .mian_right_item .mian_right_list .mian_right_list_img .mian_right_list_img_footer {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.5);
  width: 270rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 50rpx;
}

.mian .mian_right .mian_title {
  display: flex;
  margin-left: 15rpx;
  margin-bottom: 25rpx;
  margin-top: 10rpx;
}

.mian .mian_right .mian_title image {
  width: 23rpx;
  height: 28rpx;
  margin-top: 2rpx;
  margin-left: 20rpx;
}

.mian .mian_right .mian_title text {
  height: 31rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: rgba(226, 85, 32, 1);
  line-height: 31rpx;
}

.mian .mian_right .mian_right_item1 {
  width: 100%;
  padding-left: 15rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.mian .mian_right .mian_right_item1 .mian_right_list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.mian .mian_right .mian_right_item1 .mian_right_list .mian_right_list_img {
  position: relative;
  margin-bottom: 40rpx;
  width: 270rpx;
  height: 128rpx;
}

.mian .mian_right .mian_right_item1 .mian_right_list .mian_right_list_img image {
  width: 270rpx;
  height: 128rpx;
}

.mian .mian_right .mian_right_item1 .mian_right_list .mian_right_list_img .mian_right_list_img_footer {
  position: absolute;
  bottom: 52rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  height: 25rpx;
}

.icu {
  position: fixed;
  top: 90rpx;
  left: 0;
  width: 750rpx;
  background: #fff;
  padding-top: 25rpx;
}

.icu .icu_title {
  position: relative;
  text-align: center;
  height: 34rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  line-height: 34rpx;
}

.icu .icu_title image {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 0;
  right: 44rpx;
}

.icu .icu_item {
  width: 690rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  margin-top: 30rpx;
}

.icu .icu_item .icu_list {
  width: 210rpx;
  height: 78rpx;
  background: rgba(248, 248, 248, 1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 78rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(57, 57, 57, 1);
  margin-bottom: 20rpx;
  margin-right: 30rpx;
}

.icu .icu_item .icu_list1 {
  width: 210rpx;
  height: 78rpx;
  background:rgba(226,85,32,1);
  border-radius: 8rpx;
  text-align: center;
  line-height: 78rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #fff;
  margin-bottom: 20rpx;
  margin-right: 30rpx;
}
.icu .icu_item view:nth-child(3n){
  margin-right: 0;
}
