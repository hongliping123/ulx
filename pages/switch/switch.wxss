.top {
  position: relative;
  width: 694rpx;
  height: 76rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}

.top input {
  width: 694rpx;
  height: 76rpx;
  background: rgba(239, 239, 239, 1);
  border-radius: 38rpx;
  margin-top: 25rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
}

.top image {
  width: 28rpx;
  height: 30rpx;
  position: absolute;
  left: 32rpx;
  top: 23rpx;
}

.item {
  display: flex;
  width: 631rpx;
  margin-left: 36rpx;
  justify-content: space-between;
  padding-bottom: 25rpx;
  border-bottom: 1px solid #ccc;
}

.item text {
  margin-top: 20rpx;
}

.item .item_btn {
  width: 170rpx;
  height: 70rpx;
  background: rgba(226, 85, 32, 1);
  border-radius: 35rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.item1 {
  width: 631rpx;
  height: 200rpx;
  margin-left: 36rpx;
  border-bottom: 1px solid #ccc;
}

.item1 .item1_title {
  height: 31rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 31rpx;
  margin-top: 40rpx;
  margin-bottom: 38rpx;
}

.item1 .item_list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.item1 .item_list .list {
  width: 170rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  margin-bottom: 30rpx;
}

.cb {
  width: 48rpx;
  position: fixed;
  right: 30rpx;
  top: 154rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(226, 85, 32, 1);
  text-align: center;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.cb text{
  height: 23rpx;
  line-height: 23rpx;
  margin-bottom: 16rpx;
  width: 100%;
}
