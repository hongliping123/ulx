page {
  background: #f1f1f1;
}

.item {
}

.item .list {
  background: #fff;
  width: 750rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-top: 25rpx;
  margin-bottom: 20rpx;
  padding-bottom: 25rpx;
}

.item .list .list_title {
  font-size: 32rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
}

.item .list .list_content {
  display: flex;
  margin-top: 30rpx;
}

.item .list .list_content .list_content_fr {
  width: 150rpx;
  height: 27rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(29, 32, 35, 1);
  line-height: 27rpx;
  margin-top: 4rpx;
}

.item .list .list_content .list_content_fl {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
